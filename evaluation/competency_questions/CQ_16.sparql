PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX hpvco: <http://www.semanticweb.org/HPVCO:>

SELECT DISTINCT ?label
WHERE {

    hpvco:00000049 rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty hpvco:00000074 ;
        owl:someValuesFrom ?drivers
    ] .

    ?driver rdfs:subClassOf ?drivers .
    OPTIONAL {
    
        ?causalConcept rdfs:subClassOf [
        rdf:type owl:Restriction ;
        owl:onProperty hpvco:00000056 ;
        owl:someValuesFrom ?driver
    ] .
    }
    { ?driver rdfs:label ?label }
    UNION
    { ?causalConcept rdfs:label ?label }
}